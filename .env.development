# Development Environment Variables
# Override default values for local development

# Build Configuration
BUILD_ENV=development
BUILD_LOG_LEVEL=debug

# Local Development Settings
DEV_PORT=3000
DEV_HOST=localhost
DEV_WATCH=true
DEV_HOT_RELOAD=true

# Local Registry (for testing)
CI_REGISTRY=localhost:5000
CI_REGISTRY_IMAGE=${PROJECT_NAME}-dev

# Debug Settings
DEBUG=true
VERBOSE=true